{
    "configuration" : {
        "status" : "WARN",
        "name" : "SpringExamples",
        "ThresholdFilter": {
            "level": "debug"
        },
        "appenders" : {
            "Console": {
                "name": "STDOUT",
                "PatternLayout": {
                    "pattern": "%d{HH:mm:ss.SSS} [%t] %level %c - %msg%n"
                }
            }
        },
        "loggers": {
            "logger": {
                "name": "org.avlasov.springexamples",
                "level": "INFO",
                "additivity": "false",
                "AppenderRef": {
                    "ref": "STDOUT"
                }
            },
            "root": {
                "level": "error",
                "additivity": "false",
                "AppenderRef": {
                    "ref": "STDOUT"
                }
            }
        }
    }
}